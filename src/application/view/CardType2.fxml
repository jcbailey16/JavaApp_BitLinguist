            <Button fx:id="switchPic" alignment="CENTER" contentDisplay="CENTER" layoutX="199.0" layoutY="134.0" mnemonicParsing="false" onAction="#goToPicture" stylesheets="@../application.css" text="Visual" textAlignment="CENTER">
